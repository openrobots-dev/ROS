<launch>
 <!-- joy node -->
  <node respawn="true" pkg="joy"
        type="joy_node" name="joy_node" >
    <param name="dev" type="string" value="/dev/input/js0" />
    <param name="deadzone" value="0.1" />
  </node>
  
 <!-- triskar teleop node-->
  <node pkg="triskar" type="teleop_joy.py" name="teleop_joy"/>

</launch>
